    "@cdktf/provider-template": "^9.0.0",
    "cdktf": "^0.19.0",
    "constructs": "^10.1.136",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^20.4.0",
    "cdktf-cli": "^0.19.0",
    "jest": "^29.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
